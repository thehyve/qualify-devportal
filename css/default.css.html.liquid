body .api-docs-wrap ul#apidocs-resources li.resource li.operation div.apidocs-heading {
    background: none repeat scroll 0 0 #505b63;
    border-bottom: 1px solid #f9f9f9;
    border-radius: 0;
}

body .api-docs-wrap ul#apidocs-resources li.resource li.operation div.apidocs-heading:hover {
    background: none repeat scroll 0 0 #505b63 ;
}





body .api-docs-wrap ul#apidocs-resources li.resource li.operation:last-child div.apidocs-heading {
    border-bottom: 0 !important;
}

ul.operations {
    border-radius: 3px;
    box-shadow: 0 0 25px rgba(0, 0, 0, 0.25);
    overflow: hidden;
