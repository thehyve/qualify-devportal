}

body #apidocs-resources-container > h2::after {
    left: 0.5em;
    margin-right: -50%;
}

body #apidocs-resources-container > h2::before {
    margin-left: -50%;
    right: 0.5em;
}

body #apidocs-resources-container > h2 {
    color: #777;
    margin: 0 60px !important;
    overflow: hidden;
    text-align: center;
}

